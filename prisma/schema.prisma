generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model course {
  id      String @id @db.VarChar(36)
  name    String @unique @db.VarChar(100)
  acronym String @unique @db.VarChar(6)
}

model student {
  id      String @id @db.VarChar(36)
  name    String @db.VarChar(100)
  age     Int
  address String @db.VarChar(100)
}